        <open.mbprod.service.version>1.0.0</open.mbprod.service.version>
        <open.mbprod.integration.version>1.0.0</open.mbprod.integration.version>
        <open.mbprod.facade.version>1.0.0</open.mbprod.facade.version>
        <open.mbprod.web.version>1.0.0</open.mbprod.web.version>
        <open.core.facade.version>1.0.0</open.core.facade.version>
        <!-- 阿里的jar版本管理 -->
        <fastjson.version>1.2.51</fastjson.version>
        <!-- 第三方jar的版本管理 -->
        <spring.version>4.2.8.RELEASE</spring.version>
        <spring.security.version>4.2.2.RELEASE</spring.security.version>
        <spring.security.core.tiger>2.0.8.RELEASE</spring.security.core.tiger>
        <spring.oauth2.version>2.1.0.RELEASE</spring.oauth2.version>
        <spring.boot.version>1.5.2.RELEASE</spring.boot.version>
        <spring.rabbit.version>1.7.1.RELEASE</spring.rabbit.version>
        <spring.data.redis.version>1.8.1.RELEASE</spring.data.redis.version>
        <redis.clients.version>2.9.0</redis.clients.version>

        <servlet.api.version>3.1.0</servlet.api.version>
        <commons.lang.version>2.6</commons.lang.version>
        <slf4j.api.version>1.7.21</slf4j.api.version>
        <slf4j.log4j12.version>1.7.21</slf4j.log4j12.version>
        <velocity.version>1.6.2</velocity.version>
        <velocity.tools.version>2.0</velocity.tools.version>
        <druid-version>1.0.9</druid-version>
        <mybatis.version>3.2.7</mybatis.version>
        <mybatis.spring.version>1.2.2</mybatis.spring.version>
        <mysql.version>5.1.21</mysql.version>

        <aspectj.version>1.7.4</aspectj.version>
        <zkclient.version>0.10</zkclient.version>
        <dubbo.version>2.5.3</dubbo.version>
        <junit.version>4.12</junit.version>
    </properties>

    <dependencyManagement>
        <dependencies>
            <!-- 【架构体系内的jar start】 -->
            <dependency>
                <groupId>com.sm</groupId>
                <artifactId>open-care-core</artifactId>
                <version>${open.care.core.version}</version>
            </dependency>
            <dependency>
                <groupId>com.sm</groupId>
                <artifactId>open-mbprod-common</artifactId>
                <version>${open.mbprod.common.version}</version>
            </dependency>
            <dependency>
                <groupId>com.sm</groupId>
                <artifactId>open-mbprod-model</artifactId>
                <version>${open.mbprod.model.version}</version>
            </dependency>
            <dependency>
                <groupId>com.sm</groupId>
                <artifactId>open-mbprod-dal</artifactId>
                <version>${open.mbprod.dal.version}</version>
            </dependency>
            <dependency>
                <groupId>com.sm</groupId>
                <artifactId>open-mbprod-service</artifactId>
                <version>${open.mbprod.service.version}</version>
            </dependency>
            <dependency>
                <groupId>com.sm</groupId>
                <artifactId>open-mbprod-integration</artifactId>
                <version>${open.mbprod.integration.version}</version>
            </dependency>
            <dependency>
                <groupId>com.sm</groupId>
